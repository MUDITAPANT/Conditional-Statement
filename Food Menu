#include<stdio.h>
int main()
{
int choice=0;
printf("Pick your dish :\n1.Dosa\n2.Idli\n3.Vadapaw\n4.Rasam");
scanf("%d";&choice);

switch(choice)
{
case 1:
printf("Your dish is Dosa\n Price:60");
break;
case 2:
printf("Your dish is Idli\n Price:80");
break;
case 3:
printf("Your dish is Vadapaw\n Price:90");
break;
case 4:
printf("Your dish is Rasam\n Price:60");
break;
default:
printf("invalid choice");
}
return 0;
}
